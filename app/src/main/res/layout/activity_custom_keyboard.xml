<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CustomKeyboardActivity">

    <TableLayout
        android:id="@+id/tabloLayout2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline8">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.333"
            android:baselineAlignedChildIndex="0">

            <Button
                android:id="@+id/q"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="q" />

            <Button
                android:id="@+id/w"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="w" />

            <Button
                android:id="@+id/e"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="e" />

            <Button
                android:id="@+id/r"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="r" />

            <Button
                android:id="@+id/t"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="t" />

            <Button
                android:id="@+id/y"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="y" />

            <Button
                android:id="@+id/u"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="u" />

            <Button
                android:id="@+id/i"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="i" />

            <Button
                android:id="@+id/o"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="o" />

            <Button
                android:id="@+id/p"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="p" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.333"
            android:baselineAlignedChildIndex="1">

            <Button
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="a" />

            <Button
                android:id="@+id/s"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="s" />

            <Button
                android:id="@+id/d"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="d" />

            <Button
                android:id="@+id/f"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="f" />

            <Button
                android:id="@+id/g"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="g" />

            <Button
                android:id="@+id/h"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="h" />

            <Button
                android:id="@+id/j"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="j" />

            <Button
                android:id="@+id/k"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="k" />

            <Button
                android:id="@+id/l"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.111"
                android:text="l" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.333"
            android:baselineAlignedChildIndex="2">

            <Button
                android:id="@+id/enter"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.15"
                android:text="E" />

            <Button
                android:id="@+id/z"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="z" />

            <Button
                android:id="@+id/x"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="x" />

            <Button
                android:id="@+id/c"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="c" />

            <Button
                android:id="@+id/v"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="v" />

            <Button
                android:id="@+id/b"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="b" />

            <Button
                android:id="@+id/n"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="n" />

            <Button
                android:id="@+id/m"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:text="m" />

            <Button
                android:id="@+id/backspace"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.15"
                android:text="=" />

        </TableRow>

    </TableLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.74" />
</androidx.constraintlayout.widget.ConstraintLayout>